{
  "NOMBRE": {
    "default" : "",
    "type" : "Input",
    "validate" : {
      "required" : true,
      "errorEmpty" : "Nombre no puede estar vacio",
      "pattern" : "^.{2,40}$",
      "errorPattern" : "Debe estar entre 2 y 40 caracteres"
    },
    "binding" : "Materialname"
  },
  "CÓDIGO": {
    "default" : "",
    "type" : "Input",
    "validate" : {
      "required" : true,
      "errorEmpty" : "Codigo no puede estar vacio",
      "pattern" : "^[0-9]{8}$",
      "errorPattern" : "8 digitos",
      "unique" : true,
      "errorUnique" : "Duplicado"
    },
    "binding" : "Materialcode"
  },
  "UNIDAD DE MEDIDA": {
    "default" : "",
    "type" : "Input",
    "validate" : {
      "required" : true,
      "errorEmpty" : "No puede ser vacio",
      "pattern" : "^[A-Z0-9]{2,3}$",
      "errorPattern" : "Código de max. 3 caracteres alfanuméricos en mayúsculas."
    },
    "binding" : "Unit"
  },
  "DESCRIPCIÓN": {
    "default" : "",
    "type" : "Input",
    "validate" : {
      "required" : false,
      "errorEmpty" : "",
      "pattern" : "^.{0,250}$",
      "errorPattern" : "Maximo 250 caracteres"
    },
    "binding" : "Descrip"
  }
}