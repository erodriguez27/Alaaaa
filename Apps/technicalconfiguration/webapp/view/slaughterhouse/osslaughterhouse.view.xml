<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="technicalConfiguration.controller.osslaughterhouse">
	<App>
		<pages>
			<sap.m.semantic:DetailPage busy="{util>/busy}" xmlns:sap.m.semantic="sap.m.semantic" title="{i18n>OS.entity.details}" id="parameterPage" showNavButton="{util>/phone}" navButtonPress="onNavMaster">
				<sap.m.semantic:content>
					<core:Fragment fragmentName="technicalConfiguration.view.slaughterhouse.osslaughterhouse_Header" type="XML"/>
					<core:Fragment fragmentName="technicalConfiguration.view.slaughterhouse.osslaughterhouse-Table" type="XML"/>
				</sap.m.semantic:content>
				<sap.m.semantic:customFooterContent>
					<Button text="{i18n>new}" width="100px" id="newBtn" type="Accept" visible="{OSSLAUGHTERHOUSE>/new}" press="onNewRecord"/>
				</sap.m.semantic:customFooterContent>
			</sap.m.semantic:DetailPage>
		</pages>
	</App>
</mvc:View>
